nickname: "Hubbot"
realname: "Hubbot"
username: "Hubbot"

# The command character for the bot, used to trigger its commands.
command_char: "+"

# The default behavior is to load all modules in the Modules folder on startup.
# Setting modules to anything other than ["all"] causes the bot to only load the listed modules on startup
# The modules list can be defined manually, and - can be used to list modules not to load.
modules: ["all", "-MemoryHeavyModule"]

# The address to connect to
address: "irc.example.org"
# The port to use
port: 6667
# (optional) The password to user when connecting
password: "butts"
# The list of channels to join once connected
channels: ["#example"]
